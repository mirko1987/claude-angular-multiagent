# .env.example

# Anthropic API Configuration
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Default model - Claude Sonnet 4 (latest and most capable)
# Options: claude-sonnet-4-20250514 (recommended), claude-3-5-sonnet-20241022, claude-3-haiku-20240307
DEFAULT_MODEL=claude-sonnet-4-20250514

# Cache Configuration
CACHE_ENABLED=true
REDIS_HOST=localhost
REDIS_PORT=6379

# Execution Settings
PARALLEL_EXECUTION=true
# Max concurrent agents (recommended: 3 for balanced performance)
# 1 = sequential (slowest, safest)
# 3 = balanced (recommended)
# 6 = all parallel (fastest, may hit rate limits)
MAX_CONCURRENT_AGENTS=3

# Optimization Settings
OPTIMIZE=false
ALL_AGENTS=false

# Smart Orchestrator Settings
COST_BUDGET=0.50
TIME_BUDGET=90000
ENABLE_EARLY_STOP=true
ENABLE_SMART_SELECTION=true
PRIORITIZE_COST=true
PRIORITIZE_SPEED=false

# Logging
DEBUG=false
LOG_LEVEL=info